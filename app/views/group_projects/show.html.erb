<p id="notice"><%= notice %></p>

<h1><%= @group_project.group_name %> Details</h1>

<table>
  <tbody>
    <tr>
      <h1><%= @group_project.group_name %></h1>
    </tr>
  </tbody>
</table>

<% @group_project.students.each do |student| %>
<%= "#{student.first_name} #{student.last_name}" %>
<br/>
<% end %>

<% @group_project.tasks.each do |task| %>
<%= link_to task.name, group_project_task_path(task) %>
<br/>
<% end %>

<br>
